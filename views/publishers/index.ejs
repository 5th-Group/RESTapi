<h2>Publisher list</h2>
<h3>Search Bar</h3>
<form action="/books" method="GET">
    <label>Name</label>
    <input type="text" name="name" value="<%= searchDetail.name%>">
    <button type="submit">Search</button>
</form>
<br>
<br>
<% publishers.forEach(publisher => { %>
    <div href><a href="#">><%= publisher.title %></a</div>
<% })%> 