<h2>List of Cover Type</h2>
<h3>Search Bar</h3>
<form action="/covers" method="GET">
    <label>Name</label>
    <input type="text" name="type" value="<%= searchDetail.type%>">
    <button type="submit">Search</button>
</form>
<a href="/covers/new"><input type="button" value="Add Cover"></a>
<br>
<br>
<% bookCovers.forEach(cover => { %>
    <div href><a href="./covers/<%=cover._id%>"><%= cover.type %></a</div>
<% })%> 