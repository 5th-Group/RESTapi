<h2>Books list</h2>
<h3>Search Bar</h3>
<form action="/books" method="GET">
    <label>Name</label>
    <input type="text" name="name" value="<%= searchDetail.name%>">
    <button type="submit">Search</button>
</form>
<br>
<br>
<% books.forEach(book => { %>
    <div href><a href="./books/<%=book._id%>">><%= book.title %></a</div>
<% })%> 